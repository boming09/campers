   <update id="changeInfoModify" parameterType="com.cp.campers.member.model.vo.Member">
		UPDATE MEMBER
		   SET EMAIL = #{ email }
		     , USER_PHONE = #{ phone }
		     , NICKNAME = #{ nickName }
		WHERE USER_NO = #{ userNo }
	</update>
	
	<!-- 비밀번호 변경 미구현 -->
	<update id="changeInfoPwdModify" parameterType="com.cp.campers.member.model.vo.Member">
		UPDATE MEMBER
		   SET USER_PWD = #{ pwd }		     
		 WHERE USER_NO = #{ userNo }
		   AND USER_PWD = #{ pwd }
	</update>
   
