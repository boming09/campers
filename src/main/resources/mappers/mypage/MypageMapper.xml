   <select id="findMemberById" resultMap="memberResultMap">
   	SELECT
		    A.USER_NO,
		    A.USER_ID, 
		    A.USER_PWD, 
		    A.USER_NAME, 
		    A.NICKNAME, 
		    A.PROFILE_PATH, 
		    A.EMAIL, 
		    A.USER_PHONE, 
		    A.STATUS, 
		    A.CREATE_DATE, 
		    A.MODIFY_DATE,
		    B.USER_NO REF_MEMBER_NO,
		    B.AUTHORITY_CODE REF_AUTHORITY_CODE, 
		    C.AUTHORITY_CODE REF_AUTHORITY_CODE2, 
		    C.AUTHORITY_NAME, 
		    C.AUTHORITY_DESC
	    FROM MEMBER A
	    LEFT JOIN AUTHORITY_ROLE B ON(A.USER_NO = B.USER_NO)
	    LEFT JOIN AUTHORITY C ON(B.AUTHORITY_CODE = C.AUTHORITY_CODE)
    WHERE A.USER_ID = #{ userId }
    AND STATUS = 'Y'
   </select>
          
   <select id="selectMember" resultType="com.cp.campers.member.model.vo.Member">
   		SELECT *
   		FROM MEMBER
   </select>  
     
   <select id="getListCount" resultType="_int">
   		SELECT
		      COUNT(A.USER_NO)
		 FROM MEMBER A
		 LEFT JOIN AUTHORITY_ROLE B ON(A.USER_NO = B.USER_NO)
		 LEFT JOIN AUTHORITY C ON(B.AUTHORITY_CODE = C.AUTHORITY_CODE)
   </select>  
   <!-- resultType="com.cp.campers.member.model.vo.Member" -->         
   <select id="findAllMember" resultMap="memberResultMap">
   	  SELECT *
      FROM (SELECT ROWNUM RNUM, A.*
      FROM (select 
   	  		 USER_NO
   	  	   , USER_ID
   	  	   , USER_PWD
   	  	   , USER_NAME
   	  	   , NICKNAME
   	  	   , PROFILE_PATH
   	  	   , EMAIL
   	  	   , USER_PHONE
   	  	   , STATUS
   	  	   , CREATE_DATE
   	  	   , MODIFY_DATE
      FROM MEMBER
      WHERE STATUS = 'Y'
        AND USER_NO = #{ userNo }
   ORDER BY USER_NO DESC) A)
   </select>
          
   <select id="findAllBoard" resultMap="boardResultMap">
   	SELECT *
   	FROM (SELECT ROWNUM RNUM, A.*
   	FROM (SELECT
   		   B.BID
   		 , B.BTITLE
   		 , B.BCONTENT
   		 , B.MODIFY_DATE
   		 , B.CREATE_DATE
   		 , B.STATUS
   		 , B.WRITER
   		 , B.BCOUNT
   		 , B.BLIKE
   		 , NEXTID
   		 , NEXTTITLE
   		 , RREVID
   		 , PREVTITLE
   	 FROM BOARD B
   	 JOIN MEMBER M ON (B.WRITER = M.USER_NO)
   	 WHERE STATUS = 'N'
   	   AND WRITER = #{ writer }
   	 ORDER BY BID DESC) A)
   </select>
     
   <insert id="insertCamp" parameterType="com.cp.campers.mypage.model.vo.Camp">
   	INSERT 
   		INTO CAMP
		(
			CAMP_NO
			, CAMP_NAME
			, USER_NO
			, CAMP_ADDRESS
			, CAMP_PHONE
			, CAMP_INTRO
			, CAMP_PATH
			, CAMP_POLICY
			, CAMP_STATUS
			, ACCOUNT_NUM
			, BANK
			, CHECKIN
			, CHECKOUT
		)
		VALUES
		(
			SEQ_CAMP_NO.NEXTVAL
			, #{ campName }
			, #{ userNo }
			, #{ campAddress }
			, #{ campPhone }
			, #{ campIntro }
			, #{ campPath }
			, #{ campPolicy }
			, DEFAULT
			, #{ accountNum }
			, #{ bank }
			, #{ checkin }
			, #{ checkout }
		)
   </insert>
   
