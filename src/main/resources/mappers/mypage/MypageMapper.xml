 	<select id="selectMyBoardList" resultMap="boardResultMap">
	SELECT *
	FROM (SELECT ROWNUM RNUM, A.*
	FROM (select 
	            bid, 
	            btitle, 
	            bcontent, 
	            b.modify_date, 
	            b.create_date,
	            nickName,
	            bcount,
	            blike
	      from board b
	      join member m on (b.writer = m.user_no)
	      where b.status = 'N'
	      and writer = #{ writer }
	      ORDER BY bid DESC) A)
	where RNUM between #{pi.startRow} and #{pi.endRow}
	</select>
 
